<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trust.tournamentdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.trust.tournamentdemo.activiy.MyApplication"
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TournamentX">
        <activity android:name="com.trust.tournamentdemo.activiy.FreeRedeemCodeActivity"></activity>
        <activity android:name="com.trust.tournamentdemo.activiy.WinnerActivity" />
        <activity android:name="com.trust.tournamentdemo.activiy.RegistrationOtpActivity" />
        <activity android:name="com.trust.tournamentdemo.activiy.TournamentDetailActivity" />
        <activity android:name="com.trust.tournamentdemo.activiy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.trust.tournamentdemo.activiy.FirebasePushNotificationClass">
            <intent-filter>
                <action android:name="android.intent.action.RESPOND_VIA_MESSAGE" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/AppId" />

    </application>

</manifest>